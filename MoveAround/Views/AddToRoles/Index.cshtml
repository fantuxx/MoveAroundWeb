
@{
    ViewData["Title"] = "Visi Vartotojai";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}
<link href="~/lib/datatables/css/dataTables.bootstrap4.css" rel="stylesheet" />

<div class="container-fluid bg-tamsus py-2 my-2">
    <h2 class=" text-light text-center">Vartotojų duomenys ir rolės</h2>
</div>
<div class="container-fluid">
    <div style="width: 100%; margin:0 ">
        <table id="AppUsers" class="table table-striped table-bordered dt-responsive nowrap table-responsive-md" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Vardas</th>
                    <th>Pavardė</th>
                    <th>Įmonės pav.</th>
                    <th>Miestas</th>
                    <th>Email</th>
                    <th>Parinktys</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/lib/jquery.googlemap/jquery.googlemap.min.js"></script>
    <script src="~/lib/datatables/js/jquery.dataTables.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.js"></script>
    <script src="~/js/LoadUsers.js"></script>

}