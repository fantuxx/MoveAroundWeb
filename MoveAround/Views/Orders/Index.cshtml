


@{
    ViewData["Title"] = "Mano Pateikti užsakymai";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}


<div class="container-fluid bg-tamsus py-2 my-2">
    <h2 class=" text-light text-center">Mano Užsakymai</h2>
</div>
<div>
    <div style="width: 100%; ">
        <table id="Orders" class="table table-striped table-bordered  table-responsive table-responsive-xl table-responsive-lg table-responsive-md table-responsive-sm " width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Pasikrovimo vieta</th>
                    <th>Pasikrovimo data</th>
                    <th>Išsikrovimo vieta</th>
                    <th>Išsikrovimo data</th>
                    <th>Tipas</th>
                    <th>Pakrovimo būdas</th>
                    <th>Svoris</th>
                    <th>Palečių skaičius</th>
                    <th>Tūris</th>
                    <th>Ilgis</th>
                    <th>Temperatūra</th>
                    <th>Kaina</th>
                    <th>Komentaras</th>
                    <th>Id</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{

    <script src="~/js/OrdersJS/MyOrders.js"></script>

}
