@model MoveAround.Models.AppUser

@{
    ViewData["Title"] = "Refdaguoti vartotoją";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}



<div class="container-fluid bg-tamsus py-2 my-2">
    <h2 class=" text-light text-center">Redaguoti vartotją</h2>
</div>
<hr />
<form asp-action="Edit">
    <div class="row">
        <div class="col-md-4">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />

            <div class="form-group">
                <label asp-for="AccountNumber" class="control-label"></label>
                <input asp-for="AccountNumber" class="form-control" />
                <span asp-validation-for="AccountNumber" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="User.Email" class="control-label"></label>
                <input asp-for="User.Email" class="form-control" />
                <span asp-validation-for="User.Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FirstName" class="control-label"></label>
                <input asp-for="FirstName" class="form-control" />
                <span asp-validation-for="FirstName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LastName" class="control-label"></label>
                <input asp-for="LastName" class="form-control" />
                <span asp-validation-for="LastName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PhoneNumber" class="control-label"></label>
                <input asp-for="PhoneNumber" class="form-control" />
                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="User" class="control-label"></label>
                <input asp-for="User" class="form-control" />
            </div>
            <div class="form-group" hidden>
                <label asp-for="IdentityUserId" class="control-label"></label>
                <input asp-for="IdentityUserId" class="form-control" />
            </div>


        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="AddressCity" class="control-label"></label>
                <input asp-for="AddressCity" class="form-control" />
                <span asp-validation-for="AddressCity" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AddressGatveNr" class="control-label"></label>
                <input asp-for="AddressGatveNr" class="form-control" />
                <span asp-validation-for="AddressGatveNr" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BuisnessName" class="control-label"></label>
                <input asp-for="BuisnessName" class="form-control" />
                <span asp-validation-for="BuisnessName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BuisnessCode" class="control-label"></label>
                <input asp-for="BuisnessCode" class="form-control" />
                <span asp-validation-for="BuisnessCode" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="PVM" class="control-label"></label>
                <input asp-for="PVM" class="form-control" />
                <span asp-validation-for="PVM" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BuisnesAdressCity" class="control-label"></label>
                <input asp-for="BuisnesAdressCity" class="form-control" />
                <span asp-validation-for="BuisnesAdressCity" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BuisnesAdressStreet" class="control-label"></label>
                <input asp-for="BuisnesAdressStreet" class="form-control" />
                <span asp-validation-for="BuisnesAdressStreet" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EstablishedDate" class="control-label"></label>
                <input asp-for="EstablishedDate" class="form-control" />
                <span asp-validation-for="EstablishedDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BuisnessEmail" class="control-label"></label>
                <input asp-for="BuisnessEmail" class="form-control" />
                <span asp-validation-for="BuisnessEmail" class="text-danger"></span>
            </div>
        </div>

    </div>
    <div class="form-group">
        <input type="submit" value="Atnaujinti" class="btn btn-danger btn-block" />
    </div>
</form>

<div>
    <a class="btn btn-dark btn-block" asp-action="Index">Grįžti prie sąrašo</a>
</div>

