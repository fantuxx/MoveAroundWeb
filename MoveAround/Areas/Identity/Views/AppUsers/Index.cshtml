@model IEnumerable<MoveAround.Models.AppUser>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AccManagement.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tipas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccountNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
          
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddressCity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddressGatveNr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BuisnessName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BuisnessCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PVM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BuisnesAdressCity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BuisnesAdressStreet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstablishedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BuisnessEmail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tipas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccountNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddressCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddressGatveNr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BuisnessName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BuisnessCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PVM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BuisnesAdressCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BuisnesAdressStreet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstablishedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BuisnessEmail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
