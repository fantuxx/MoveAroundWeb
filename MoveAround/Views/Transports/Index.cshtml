@model IEnumerable<MoveAround.Models.Transport>

@{
    ViewData["Title"] = "Mano Pateiktas transportas";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}



<div class="container-fluid bg-tamsus py-2 my-2">
    <h2 class=" text-light text-center">Mano Pateiktas transportas</h2>
</div>
<div>
    <div style="width: 100%; ">
        <table id="MyTransports" class="table table-striped table-bordered  table-responsive table-responsive-xl table-responsive-lg table-responsive-md table-responsive-sm " width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Adresas iš kur</th>
                    <th>Galima nuo</th>
                    <th>Pakrovimo tipas</th>
                    <th>Sunkvežimio tipas</th>
                    <th>Sunkvežimių sk.</th>
                    <th>Palečių sk.</th>
                    <th>Tūris</th>
                    <th>Svoris (kg)</th>
                    <th>Reguliuojam temperatūra</th>
                    <th>Ilgis</th>
                    <th>Kaina</th>
                    <th>Komentaras</th>
                    <th>Id</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{

    <script src="~/js/TransportJS/LoadMytransports.js"></script>
}